{
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "display": "standalone",
  "scope": "/",
  "scope_url": "https://aaron-pwa.netlify.app",
  "start_url": "/",
  "intent_filters": {
    "scope_url_scheme": "https",
    "scope_url_host": "aaron-pwa.netlify.app",
    "scope_url_path": "/"
  },
  "name": "Aaron PWA",
  "short_name": "Aaron PWA",
  "description": "A Demo for Next-PWA",
  "url_handlers": [
    {
      "origin": "https://aaron-pwa.netlify.app"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "aaronpwa",
      "url": "/%s"
    }
  ],
  "icons": [
      {
          "src": "/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
      },
      {
          "src": "/icon-256x256.png",
          "sizes": "256x256",
          "type": "image/png"
      },
      {
          "src": "/icon-384x384.png",
          "sizes": "384x384",
          "type": "image/png"
      },
      {
          "src": "/icon-512x512.png",
          "sizes": "512x512",
          "type": "image/png"
      }
  ]
}